{
  "name": "fairybread",
  "version": "2.0.0",
  "description": "a javascript utility to manage css and match sass functions",
  "main": "dist/fairybread.js",
  "scripts": {
    "pretest": "tsc ./src/fairybread.ts --outFile ./dist/fairybread.js",
    "test": "jest",
    "build:docs": "npm run test && tsc ./src/fairybread.ts --outFile ./docs/fairybread.js",
    "build:release": "npm run test",
    "docs": "npm run test && http-server docs/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stagfoo/fairybread.git"
  },
  "keywords": [
    "js",
    "sass",
    "css",
    "javascript",
    "jss"
  ],
  "author": "Alex King",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stagfoo/fairybread/issues"
  },
  "homepage": "https://github.com/stagfoo/fairybread#readme",
  "devDependencies": {
    "jest": "^20.0.1",
    "typescript": "^2.7.1"
  }
}
